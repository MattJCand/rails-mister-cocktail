<div class="navbar-wagon">
  <!-- Logo -->
  <%= link_to "Mr Cocktail", cocktails_path, class: "btn btn-primary"%>
</div>

<div class="container">
  <div class="row">
    <div class="col-xs-12" style="margin-top: 10%;">
      <%= simple_form_for(@cocktail) do |f| %>
      <%= f.input :name, placeholder:"Mojito"%>
      <%= f.input :photo, as: :attachinary %>
      <%= f.submit %>
      <% end %>
    </div>
  </div>
</div>
